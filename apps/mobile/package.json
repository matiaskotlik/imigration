{
  "name": "@repo/mobile",
  "version": "1.0.0",
  "private": true,
  "exports": {
    ".": "./expo-router/entry"
  },
  "scripts": {
    "android": "expo run:android",
    "build:check": "tsc --noEmit",
    "clean": "git clean -xdf .cache .expo .turbo android ios node_modules",
    "dev": "expo start",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "format": "prettier . --write --ignore-path ../../.gitignore --cache --cache-location .cache/.prettiercache",
    "format:check": "prettier . --check --ignore-path ../../.gitignore --cache --cache-location .cache/.prettiercache",
    "ios": "expo run:ios",
    "lint": "expo lint -- --fix --cache --cache-location .cache/.eslintcache",
    "lint:check": "expo lint -- --cache --cache-location .cache/.eslintcache"
  },
  "prettier": "@repo/prettier-config",
  "dependencies": {
