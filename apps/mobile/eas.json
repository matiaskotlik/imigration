{
  "cli": {
    "appVersionSource": "local"
  },
  "build": {
    "base": {
      "corepack": true
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "APP_NAME_SUFFIX": " (Dev)",
        "APP_IDENTIFIER_SUFFIX": ".dev"
      }
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "APP_NAME_SUFFIX": " (Staging)",
        "APP_IDENTIFIER_SUFFIX": ".staging"
      }
    },
    "production": {
      "extends": "base",
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
