{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "target": "es2023",
    "lib": ["dom", "dom.iterable", "es2023"],
    "allowJs": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "module": "nodenext",
    "moduleDetection": "force",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  }
}
