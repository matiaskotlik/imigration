{
  "name": "@repo/eslint-config",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./*": "./*.js"
  },
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "prettier": "@repo/prettier-config",
  "dependencies": {
    "@eslint/compat": "^1.3.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@next/eslint-plugin-next": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint-config-expo": "latest",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "latest",
    "eslint-config-turbo": "latest",
    "eslint-import-resolver-typescript": "latest",
    "eslint-plugin-canonical": "latest",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-x": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-markdown": "latest",
    "eslint-plugin-n": "latest",
    "eslint-plugin-perfectionist": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "eslint-plugin-turbo": "^2.5.4",
    "eslint-plugin-unicorn": "latest",
    "eslint-plugin-vitest": "latest",
    "globals": "^16.2.0",
    "typescript-eslint": "latest"
  },
  "devDependencies": {
    "@repo/prettier-config": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  }
}
